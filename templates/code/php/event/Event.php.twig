{% extends "code/php/common/class.php.twig" %}

{# Override the includes block to include the DateTime class if needed #}
{% block includes %}
    {%~ if model is dateTimeUsed %}
    use \DateTimeInterface;

    {%~ endif %}
{% endblock %}
{# Override the fields block to deal with data elements #}
{% block fields %}
    {%~ for data in eventHelper.getNamedData(event) %}
    {%~ include 'code/php/common/partials/_field.php.twig' with {'field': data} %}
    {%~ endfor %}
{% endblock %}
